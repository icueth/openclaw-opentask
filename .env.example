# OpenClaw Dashboard Environment Configuration
# Copy this file to .env.local and update the values

# Gateway Configuration
NEXT_PUBLIC_GATEWAY_URL=http://localhost:18789
NEXT_PUBLIC_GATEWAY_TOKEN=

# GitHub OAuth App Credentials (required for OAuth authentication)
# Create at: https://github.com/settings/developers
# Set Authorization callback URL to: http://localhost:3456/api/auth/github/callback
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Optional: Override the default port
# PORT=3456

# Optional: Override the OAuth redirect URI (defaults to http://localhost:3456/api/auth/github/callback)
# GITHUB_REDIRECT_URI=http://localhost:3456/api/auth/github/callback

# Optional: Enable debug mode
# DEBUG=true
